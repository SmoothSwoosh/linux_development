#: akinator.c:77
#, c-format
msgid "Usage: %s [-r]\n"
msgstr "Запуск: %s [-r]\n"

#: akinator.c:78
#, c-format
msgid "Guess a number from 1 to 100 answering yes or no\n"
msgstr "Отгадайте число от 1 до 100, отвечая да или нет\n"

#: akinator.c:79
#, c-format
msgid "-h, --help \t help\n"
msgstr "-h, --help \t использование\n"

#: akinator.c:80
#, c-format
msgid "-r \t\t use roman notation\n"
msgstr "-r \t\t использовать романскую нотацию\n"

#: akinator.c:90
#, c-format
msgid "Is your number greater than %s? Answer yes or no\n"
msgstr "Ваше число больше %s? Ответьте да или нет\n"

#: akinator.c:92
#, c-format
msgid "Is your number greater than %d? Answer yes or no\n"
msgstr "Ваше число больше %d? Ответьте да или нет\n"

#: akinator.c:95
msgid "yes"
msgstr "да"

#: akinator.c:103
#, c-format
msgid "Your number is %s\n"
msgstr "Ваше число %s\n"

#: akinator.c:105
#, c-format
msgid "Your number is %d\n"
msgstr "Ваше число %d\n"
